{
  "env": {
    "DEVICE_NAME": "m5stack-atom-s3r"
  },
  "configurations": [
      {
          "name": "Default",
          "includePath": [
              "${workspaceFolder}",
              "${workspaceFolder}/components/bmi270",
              "${workspaceFolder}/.venv/Lib/site-packages/esphome",
              "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/src",
              "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/.piolibdeps/${DEVICE_NAME}",
              "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/.piolibdeps/${DEVICE_NAME}/M5GFX/src",
              "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/.piolibdeps/${DEVICE_NAME}/M5Unified/src",
              ""
          ],
          "browse": {
              "limitSymbolsToIncludedHeaders": true,
              "path": [
                  "${workspaceFolder}",
                  "${workspaceFolder}/components/bmi270",
                  "${workspaceFolder}/.venv/Lib/site-packages/esphome",
                  "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/src",
                  "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/.piolibdeps/${DEVICE_NAME}",
                  "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/.piolibdeps/${DEVICE_NAME}/M5GFX/src",
                  "${workspaceFolder}/.esphome/build/${DEVICE_NAME}/.piolibdeps/${DEVICE_NAME}/M5Unified/src",
                  ""
              ]
          },
          "defines": [
              "PLATFORMIO=60118",
              "ESP8266",
              "ARDUINO_ARCH_ESP8266",
              "ARDUINO_ESP8266_NODEMCU_ESP12E",
              "ESPHOME_LOG_LEVEL=ESPHOME_LOG_LEVEL_VERY_VERBOSE",
              "USE_ARDUINO",
              "USE_ESP8266",
              "USE_ESP8266_FRAMEWORK_ARDUINO",
              "F_CPU=80000000L",
              "__ets__",
              "ICACHE_FLASH",
              "_GNU_SOURCE",
              "ARDUINO=10805",
              "ARDUINO_BOARD=\"PLATFORMIO_NODEMCUV2\"",
              "ARDUINO_BOARD_ID=\"nodemcuv2\"",
              "FLASHMODE_DIO",
              "LWIP_OPEN_SRC",
              "NONOSDK22x_190703=1",
              "TCP_MSS=536",
              "LWIP_FEATURES=1",
              "LWIP_IPV6=0",
              "VTABLES_IN_FLASH",
              "MMU_IRAM_SIZE=0x8000",
              "MMU_ICACHE_SIZE=0x8000",
              ""
          ],
          "cStandard": "gnu17",
          "cppStandard": "gnu++17"
      }
  ],
  "version": 4
}